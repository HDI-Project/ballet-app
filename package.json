{
  "name": "ballet-bot",
  "version": "1.0.0",
  "description": "An app to help with ballet automation and auxillary methods",
  "author": "kelvin-lu <kelvinlu@mit.edu> (https://github.com/HDI-Project/ballet)",
  "license": "ISC",
  "repository": "https://github.com//ballet-bot.git",
  "homepage": "https://github.com//ballet-bot",
  "bugs": "https://github.com//ballet-bot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "isomorphic-git": "^0.54.2",
    "probot": "^7.2.0",
    "request-promise": "^4.2.4",
    "tmp": "^0.1.0",
    "yaml": "^1.5.1"
  },
  "devDependencies": {
    "jest": "^24.0.0",
    "nock": "^10.0.0",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
